      <Query>
        <DataSourceName>conexion</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select m.*,
dm.Item,dm.IdArticulo,dm.Cantidad,dm.Descripcion,a.Unidad,(isnull(a.Peso,0)*dm.Cantidad)as PesoTotal from Movimiento m inner join detallemovimiento
dm on m.TipoDocumento=dm.TipoDocumento
and m.NumeroDocumento=dm.NumeroDocumento
and m.IdAlmacen=m.IdAlmacen
inner join articulo a on dm.IdArticulo=a.idarticulo
where m.TipoDocumento='GR' AND m.NumeroDocumento='T0010000001'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
