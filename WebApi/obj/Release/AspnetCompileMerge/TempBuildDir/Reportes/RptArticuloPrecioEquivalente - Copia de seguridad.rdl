      <Query>
        <DataSourceName>conexion</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select   ROW_NUMBER() OVER(PARTITION BY a.idarticulo ORDER BY a.idarticulo ASC) 
    AS Item,a.idarticulo,Descripcion1,Idgrupo,Idfamilia,IdModelo,a.IdMarca,m.marca,au.IdVenta,au.preciounitario,ae.unidaddestino,ae.Medida from articulo a
inner join tbl_articulo_unidadventa au on a.IdArticulo=au.idarticulo
inner join unidadequivalenxarticulo ae on  au.idventa=ae.ID
left join vmarca m on a.idmarca=m.idmarca</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
