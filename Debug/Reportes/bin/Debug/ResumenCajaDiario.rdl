ther><Paragraphs><Paragraph><TextRuns><TextRun><Value>=Parameters!Empresa.Value</Value><Style><FontFamily>Calibri</FontFamily></Style></TextRun></TextRuns><Style /></Paragraph></Paragraphs><rd:DefaultName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Empresa</rd:DefaultName><Left>0cm</Left><Height>0.6cm</Height><Width>8.89cm</Width><ZIndex>2</ZIndex><Style><Border><Style>None</Style></Border><PaddingLeft>2pt</PaddingLeft><PaddingRight>2pt</PaddingRight><PaddingTop>2pt</PaddingTop><PaddingBottom>2pt</PaddingBottom></Style></Textbox></ReportItems><Style><Border><Style>None</Style></Border></Style></PageHeader><PageFooter><Height>0.05292cm</Height><PrintOnFirstPage>true</PrintOnFirstPage><PrintOnLastPage>true</PrintOnLastPage><Style><Border><Style>None</Style></Border></Style></PageFooter><PageHeight>29.7cm</PageHeight><PageWidth>21cm</PageWidth><LeftMargin>1cm</LeftMargin><RightMargin>1cm</RightMargin><TopMargin>1cm</TopMargin><BottomMargin>1cm</BottomMargin><ColumnSpacing>1.27cm</ColumnSpacing><Style /></Page><AutoRefresh>0</AutoRefresh><DataSources><DataSource Name="conexion"><DataSourceReference>conexion</DataSourceReference><rd:SecurityType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">None</rd:SecurityType><rd:DataSourceID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">360ea760-3e86-4fcd-ae8f-460ce039b985</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="conexion"><Query><DataSourceName>conexion</DataSourceName><CommandText>SELECT '1-VENTAS' as Grupo,c.IdMoneda,sum(c.Pago) as Pago,  
sum(case when ltrim(rtrim(referencia))='1' then c.PagoMN else null end) as PagoMN,
sum(c.PagoUS) as PagoUS,c.IdMonedaPago, 
c.FechaDocumento, d.IdCaja, tc.NombreCaja,vc.Cajas
FROM Tbl_Caja_Venta AS c INNER JOIN Tbl_DetalleUsuarioCaja AS d ON c.IdDetUsuarioCaja = d.IdDetUsuarioCaja 
INNER JOIN  Tbl_Usuario_Caja_General AS tc ON d.IdCaja = tc.IdCaja INNER JOIN VTipoOperacion ON d.IdTipoOperacion =
VTipoOperacion.IdCodigo INNER JOIN  Comprobante AS CP ON c.IdAgencia = CP.IdAgencia AND c.IdTipoDocumento = 
CP.IdTipoDocumento AND c.Serie = CP.Serie AND c.NumeroDocumento = CP.NumeroDocumento AND   c.IdAlmacen = CP.IdAlmacen 
inner join vcajas vc on d.Idcaja=vc.IdCaja  where isnull(cp.estado,'V')='V' And  
(c.FechaDocumento between '10/03/2018' and '15/03/2018') and d.IdCaja in('C101','C102') 
group by c.IdMoneda,  c.IdMonedaPago, c.FechaDocumento, d.IdCaja, tc.NombreCaja,vc.Cajas   
union all 
SELECT '2-COBRANZA' as Grupo,dtc.IdMonedaO AS IdMoneda, sum(dtc.ImporteCobrado) AS Pago,
sum(case when ltrim(rtrim(referencia))='1' then dtc.ImpCobMN else null end) as PagoMN, 
sum(dtc.ImpCobUS) AS PagoUS,dtc.IdMoneda AS IdMonedaPago, dtc.FechaMovimiento AS FechaDocumento,  dtc.IdCaja, '' AS NombreCaja,vc.Cajas FROM Tbl_DetalleCobranza AS dtc 
INNER JOIN Cliente AS cl ON dtc.IdCliente = cl.IdCliente INNER JOIN Tbl_DetalleUsuarioCaja AS dtuc ON 
dtc.IdDetUsuarioCaja = dtuc.IdDetUsuarioCaja INNER JOIN  VTipoOperacion ON dtuc.IdTipoOperacion = VTipoOperacion.IdCodigo  
inner join vcajas vc on dtc.IdCaja=vc.Idcaja  left join vformapago vfp on dtc.FormaPago=vfp.IdFormaPago  
where (fechamovimiento between '01/01/2018' and '15/03/2018') and (dtuc.IdCaja in('C101','C102'))
and ltrim(rtrim(referencia))='1'
group by IdMonedaO, dtc.IdMoneda,FechaMovimiento, dtc.IdCaja,vc.Cajas
union all 
select case when caja.IdTipoMovimiento='1' Then '3-INGRESOS' else '4-EGRESOS' end  AS Grupo,'' as 
IdMoneda,sum(case when IdMoneda='MN' then importeMN*signo else IMporteMN*signo/TipoCambio end) as Pago,  
sum(ImporteMN*signo) as PagoMN,sum(importeUS*signo) as PagosUS,
IdMoneda as IdMonedaPago,FechaMovimiento as FechaDocumento,caja.IdCaja,'' as 
NombreCaja,vc.Cajas from caja  inner join vcajas vc on  caja.IdCaja=vc.IdCaja left join vcajas vco on 
 caja.idcajaorigen=vco.idcaja left join Proveedor pv on LTRIM(rtrim(IdTipoAnexo))+LTRIM(RTRIM(IdAnexo))=LTRIM(rtrim(pv.TipoAnexo))+
 LTRIM(RTRIM(pv.IdProveedor))
 where (caja.IdCaja in('C101','C102')) and (fechamovimiento between '10/03/2018' and '15/03/2018')
group by IdMoneda,IdMoneda,FechaMovimiento, caja.IdCaja,vc.Cajas,caja.IdTipoMovimiento</CommandText><rd:UseGenericDesigner xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">true</rd:UseGenericDesigner></Query><Fields><Field Name="Grupo"><DataField>Grupo</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="IdMoneda"><DataField>IdMoneda</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Pago"><DataField>Pago</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="PagoMN"><DataField>PagoMN</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="PagoUS"><DataField>PagoUS</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="IdMonedaPago"><DataField>IdMonedaPago</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="FechaDocumento"><DataField>FechaDocumento</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.DateTime</rd:TypeName></Field><Field Name="IdCaja"><DataField>IdCaja</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="NombreCaja"><DataField>NombreCaja</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Cajas"><DataField>Cajas</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="FechaInicial"><DataType>String</DataType><Nullable>true</Nullable><AllowBlank>true</AllowBlank><Prompt>FechaInicial</Prompt></ReportParameter><ReportParameter Name="FechaFinal"><DataType>String</DataType><Nullable>true</Nullable><AllowBlank>true</AllowBlank><Prompt>FechaFinal</Prompt></ReportParameter><ReportParameter Name="Empresa"><DataType>String</DataType><Nullable>true</Nullable><AllowBlank>true</AllowBlank><Prompt>Empresa</Prompt></ReportParameter></ReportParameters><Language>es-PE</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Cm</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">65adb5ce-f1b1-49f5-b841-0de097ae2216</rd:ReportID></Report>